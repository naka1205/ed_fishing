<?xml version="1.0" encoding="utf-8"?>
<Layer anchor-point="0,0">
  <!--bg-->
  <Sprite position="480,0" anchor-point="0.5,0">
    <properties>
      <property name="filename" value="ui_paused.png"/>
    </properties>
  </Sprite>
  <!--继续游戏按钮-->
  <Button position="480,100" name="continue_btn">
    <properties>
      <property name="res-type" value="XML"/>
      <property name="normal-image" value="ui_continue_game_normal.png"/>
      <property name="selected-image" value="ui_continue_game_selected.png"/>
    </properties>
  </Button>
  <!--主菜单按钮-->
  <Button position="480,255" name="main_btn">
    <properties>
      <property name="res-type" value="XML"/>
      <property name="normal-image" value="ui_main_menu_normal.png"/>
      <property name="selected-image" value="ui_main_menu_selected.png"/>
    </properties>
  </Button>
  <!--选择场景按钮-->
  <Button position="480,345" name="choose_tollgate_btn">
    <properties>
      <property name="res-type" value="XML"/>
      <property name="normal-image" value="ui_tollgate_normal.png"/>
      <property name="selected-image" value="ui_tollgate_selected.png"/>
    </properties>
  </Button>
  <!--音乐文本-->
  <Sprite position="450,440" anchor-point="0.5,0.5">
    <properties>
      <property name="filename" value="ui_music_text.png"/>
    </properties>
  </Sprite>
  <!--音乐-->
  <CheckBox position="520,440" name="music_btn">
    <properties>
      <property name="res-type" value="XML"/>
      <property name="background" value="ui_set_006.png"/>
      <property name="cross" value="ui_set_007.png"/>
    </properties>
  </CheckBox>
  <!--音乐文本-->
  <Sprite position="450,520" anchor-point="0.5,0.5">
    <properties>
      <property name="filename" value="ui_chunk_text.png"/>
    </properties>
  </Sprite>
  <!--音效-->
  <CheckBox position="520,520" name="chunk_btn">
    <properties>
      <property name="res-type" value="XML"/>
      <property name="background" value="ui_set_004.png"/>
      <property name="cross" value="ui_set_005.png"/>
    </properties>
  </CheckBox>
</Layer>